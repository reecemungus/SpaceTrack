config_version=5

[application]

config/name="SpaceTrack"
run/main_scene="res://Scenes/Levels/main_menu_level.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://Scripts/AutoLoad/AudioManager.gd"
GameStatics="*res://Scripts/AutoLoad/GameStatics.gd"
PlayerInfo="*res://Scripts/AutoLoad/PlayerInfo.gd"
SignalBus="*res://Scripts/AutoLoad/SignalBus.gd"
ParticleManager="*res://Scripts/AutoLoad/ParticleManager.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[editor]

movie_writer/mjpeg_quality=1.0
movie_writer/movie_file="W:/Videos/Video.avi"

[file_customization]

folder_colors={
"res://Assets/": "orange",
"res://Scenes/": "blue",
"res://Scripts/": "pink"
}

